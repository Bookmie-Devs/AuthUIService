model login_form {
    id            Int       @id @default(autoincrement())
    title         String?
    description   String?
    script        String?
    preview_image String?
    project       project[]
}

model signup_form {
    id            Int       @id @default(autoincrement())
    title         String?
    description   String?
    script        String?
    preview_image String?
    project       project[]
}
